{
  "$schema": "./appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "RobotsTxtOption": "Cms",
  "Umbraco": {
    "CMS": {
      "Hosting": {
        "Debug": true
      },
      "Global": {
        "Id": "b396be80-3d27-48f4-b69d-8e9170ba0a5c",
        "SanitizeTinyMce": true
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        },
        "Error404Collection": [
            {
                "Culture": "default",
                "ContentXPath": "//error"
            }
        ]
      },      
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeManual",
        "ModelsNamespace": "GradimoZajedno.Models.Generated",
        "FlagOutOfDateModels": false,
        "ModelsDirectory": "~/../GradimoZajedno.Models/Generated",
        "AcceptUnsafeModelsDirectory": true,
        "DebugLevel": 0
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Server=irenait.ddns.net;Database=GradimoZajedno.Umbraco;User Id=GradimoZajedno;Password=GradimoZajedno;",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.SqlClient"
  }
}